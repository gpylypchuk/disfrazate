import csv
from members.models import Producto  # Reemplaza 'tuapp' con el nombre de tu app

# Abrir el archivo CSV
with open('productos.csv', 'r', encoding='utf-8') as file:
    reader = csv.DictReader(file, delimiter=';')  # Usamos ; como delimitador

    # Iterar sobre cada fila del archivo CSV
    for row in reader:
        Producto.objects.create(
            id=row['idDisfraz'],
            nombre=row['descripcion'],
            stock=row['cantidad'],
            precio_venta=row['precioVenta'],
            precio_alquiler=row['precioAlquilerDia']
        )

print("Productos importados exitosamente.")